<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Person.
    -->
    <changeSet id="20170820143154-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="organization_id"
                                 baseTableName="person"
                                 constraintName="fk_person_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organization"/>

        <addForeignKeyConstraint baseColumnNames="app_account_id"
                                 baseTableName="person"
                                 constraintName="fk_person_app_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_account"/>

    </changeSet>
</databaseChangeLog>
